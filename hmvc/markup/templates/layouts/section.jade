doctype html
include /bem
html
  head
    block variables
    include ../blocks/head
  body(id='#{self.bodyId}')
    //- include ../blocks/sitetoolbar
    include ../blocks/sitetoolbar-login-loaders
    .page.page_sidebar_on
      include ../blocks/prev-next
      include ../blocks/sidebar
      .page__inner
        article.main
          header.main__header.main__header_center
            include ../blocks/breadcrumbs
            h1.main__header-title!= self.title
          block content
      include ../blocks/page-footer
    include ../blocks/scripts