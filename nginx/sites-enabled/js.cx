# Static no-cookie host
# All resources are accessible from the main host too

server {

  listen 80;
  listen 443;
  server_name js.cx static-nightly.javascript.ru static-nightly.javascript.info;

  root <%=root%>/public;

<% if (setPassword) { %>
  auth_basic "Administrator Login";
  auth_basic_user_file /etc/nginx.passwd;
<% } %>

  access_log  /var/log/nginx/js.cx.log main;
  charset utf-8;

  include "partial/javascript-static";

  add_header Access-Control-Allow-Origin *;
}
