.quiz-results-table

    .main &__results ul,
    .main &__results li,
    .main &__results dl,
    .main &__results dt,
    .main &__results dd,
    .main &__results h1,
    .main &__results p,
    .main &__results th,
    .main &__results td
    .main &__results
        font-size inherit
        font-weight normal

        margin 0
        padding 0

    .main &__results &__result:nth-child(even)
        background none

    .main &__results &__result:last-child
        border none

    &__results
        width 100%

    .main &__results tr:first-child th
        vertical-align top
        border-bottom-width: 1px

    .main &__results th,
    .main &__results td
        vertical-align top

    .main &__results td,
    .main &__results th
        padding 25px

    &__test-info,
    &__weak-list
        width 50%

    .main &__results &__time,
    .main &__results &__try,
    .main &__results &__title,
    .main &__results &__title-head
        font-size 12px
        color #727272

    .main &__results &__name
        font: 17px/19px secondary_font

        margin-top 5px

    .main &__results &__try
        margin-top 7px

    &__precents
        text-align center

    .main &__results &__precents-value
        font-size 28px
        line-height initial

        color #4C906B