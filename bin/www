#!/usr/bin/env node
'use strict';

const log = require('lib/log')(module);
const config = require('config');
const co = require('co');

co(require('app'))(function(err, app) {

  if (err) throw err;

  app.listen(config.port, config.host, function() {
    log.info('App listen %s:%d', config.host, config.port);
  });
});


